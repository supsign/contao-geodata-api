{
    "name": "supsign/attendance-list",
    "type": "symfony-bundle",
    "description": "Contao 4 bundles prototype",
    "homepage": "https://contao.org",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Supsign",
            "homepage": "https://github.com/Supsign"
        }
    ],
    "require": {
        "php": "^5.6 || ^7.0",
        "contao/core-bundle": "4.*"
    },
    "extra": {
        "contao-manager-plugin": "Supsign\\ContaoAttendanceListBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Supsign\\ContaoAttendanceListBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Supsign\\ContaoAttendanceListBundle\\Tests\\": "tests/"
        }
    },
    "support": {
        "issues": "https://github.com/SupsignGmbH/attendance-list/issues",
        "source": "https://github.com/SupsignGmbH/attendance-list"
    }
}
